<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SlideshowPageController">
   <children>
      <Text layoutX="300.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Slideshow" style="-fx-font-family: 'Times New Roman'; -fx-font-size: 18pt" />
      <BorderPane fx:id="borderPane" layoutX="170.0" layoutY="60.0" prefHeight="300.0" prefWidth="300.0" />
      <Button fx:id="previousSlideButton" layoutX="50.0" layoutY="200.0" prefHeight="50.0" prefWidth="50.0" mnemonicParsing="false" onAction="#previousSlide" text="&lt;" />
      <Button fx:id="nextSlideButton" layoutX="600.0" layoutY="200.0" prefHeight="50.0" prefWidth="50.0" mnemonicParsing="false" onAction="#nextSlide" text="&gt;" />
      <Button fx:id="returnToAlbumPageButton" layoutX="30.0" layoutY="450.0" mnemonicParsing="false" onAction="#returnToAlbumPage" text="Return to album page" style="-fx-font-family: 'Times New Roman'; -fx-font-size: 14pt" />
      <Button fx:id="logoutButton" layoutX="600.0" layoutY="450.0" mnemonicParsing="false" onAction="#logout" text="logout" style="-fx-font-family: 'Times New Roman'; -fx-font-size: 14pt" />
   </children>
</AnchorPane>
